{
  "version": "1.0",

  "name": "Postman Monitors Alerts (BitScoop Alexa Demo)",
  "url": "https://api.getpostman.com",

  "headers": {
    "X-Api-Key": "*** INSERT YOUR AUTH KEY HERE ***"
  },

  "endpoints": {
    "RunMonitor": {
      "POST": {
        "method": "POST",
        "route": {
          "path": "/monitors/{{ parameters.monitor_id }}/run"
        }
      }
    }
  }
}
